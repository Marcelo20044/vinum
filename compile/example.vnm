sdrink NORMIS = "normis"
sdrink NERD = "nerd"
sdrink b = "c++"

fun check(sdrink a) {
    beef a == NORMIS {
        a = "fuck"
        toastn(a + " " + b)
    } bells beef a == NERD{
        a = "love"
        toastn(a + " " + b)
    }
}

fun pinta factorial(pinta n) {
    beef n == 0 {
        refill 1
    }
    refill n * factorial(n - 1)
}

sdrink a = NERD
check(a)

pour pinta i = 1; i <= 5; i = i + 1 {
    toastn(factorial(i))
}
