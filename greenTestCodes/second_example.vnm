pinta a = 1 + 1
pinta b = a * 3
toast(b)
beef a > b {
    toast(a - b)
} bells {
    toast(b - a)
}
pour pinta i = 1; i <= 5; i = i + 1 {
    toast(i)
}
fun pinta check(pinta number) {
    pinta temp = 0
    pour pinta t = 1; t < number; t = t + 1 {
        temp = temp + t
    }
    refill temp
}
toast(check(7))
fun pinta factorial(pinta n) {
    beef n == 0 {
        refill 1
    }
    refill n * factorial(n - 1)
}
pour pinta r = 1; r <= 5; r = r + 1 {
    toast(factorial(r))
}