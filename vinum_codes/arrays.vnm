fun sort(pinta arr[]) {
    pour pinta i = 1; i < len(arr); i = i + 1 {
        pour pinta j = i; j > 0; j = j - 1 {
            beef arr[j - 1] > arr[j] {
                bruderschaft(arr[j - 1], arr[j])
            }
        }
    }
}

pinta arr[5] = {5, 3, 4, 2, 1}
toast("before sort: ")
pour pinta i = 0; i < 5; i = i + 1 {
    toast(arr[i] + " ")
}

sort(arr)

toast("\nafter sort: ")
pour pinta i = 0; i < 5; i = i + 1 {
    toast(arr[i] + " ")
}