pinta a = 10
pinta b = 20

sdrink str = "Сложение: "
toast(str)
toast(a + b)

toast("Вычитание: ")
toast(a - b)

toast("Умножение: ")
toast(a * b)

toast("Деление: ")
toast(b / a)

toast("Остаток: ")
toast((b + 3) % a)

pinta c = a + b - (2 * b) + (a * a * a) / ((b + 2) % a)
toast("Много операций: ")
toast(c)

